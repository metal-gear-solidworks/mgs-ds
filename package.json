{
  "name": "driver-station",
  "productName": "driver-station",
  "version": "1.0.0",
  "description": "Driver station for Metal Gear Solidworks",
  "main": "dist/index.js",
  "scripts": {
    "start": "tsc && electron-forge start --no-sandbox",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts ."
  },
  "keywords": [],
  "author": "Tyler Chen",
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "driver_station"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.49",
    "@electron-forge/maker-deb": "^6.0.0-beta.49",
    "@electron-forge/maker-rpm": "^6.0.0-beta.49",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.49",
    "@electron-forge/maker-zip": "^6.0.0-beta.49",
    "@typescript-eslint/eslint-plugin": "^2.19.2",
    "@typescript-eslint/parser": "^2.19.2",
    "electron": "8.0.1",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.1",
    "typescript": "^3.7.5"
  },
  "dependencies": {
    "@types/node": "12.12.6",
    "electron-squirrel-startup": "^1.0.0"
  }
}
